{
  "rules": {
    "no-cond-assign": 2,
    "no-console": 1,
    "no-constant-condition": 1,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-negated-in-lhs": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "no-unexpected-multiline": 2,
    "consistent-return": 1,
    "curly": [1, "multi-line"],
    "dot-location": [1, "property"],
    "eqeqeq": [2, "smart"],
    "no-fallthrough": 2,
    "no-loop-func": 2,
    "no-param-reassign": 1,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "wrap-iife": [2, "inside"],
    "yoda": [2, "never"],
    "strict": [1, "global"],
    "no-shadow-restricted-names": 2,
    "no-shadow": 1,
    "no-unused-vars": [1, {args: "none"}],
    "no-use-before-define": [2, "nofunc"],
    "array-bracket-spacing": 1,
    "block-spacing": [1, "never"],
    "brace-style": [1, "1tbs", {"allowSingleLine": true}],
    "camelcase": [1, {"properties": "never"}],
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": [1, "never"],
    "indent": [1, 2],
    "key-spacing": 1,
    "linebreak-style": [1, "unix"],
    "new-cap": 2,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-object": 2,
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-unneeded-ternary": 1,
    "quotes": [1, "double"],
    "semi-spacing": 1,
    "semi": [2, "never"],
    "keyword-spacing": 1,
    "space-before-blocks": 1,
    "space-in-parens": [1, "never"],
    "space-unary-ops": 1
  }
}
